build () {
	 add_module btrfs
	 add_module dm-crypt
	 add_module dm-mod
	 add_module dm-snapshot
	 add_module fuse
	 add_binary /bin/bash
	 add_binary /bin/date
	 add_binary /usr/bin/pstree
	 add_binary /bin/cat
	 add_binary /usr/bin/head
	 add_file /etc/hostname
	 add_runscript
}

help () {
	 cat <<-EOF
		This is my nifty universal fs-logic crap :D
		EOF
}
