#!/usr/bin/python3

import json
import os
import re
import urllib.request
import ssl

for env in ['http_proxy', 'HTTP_PROXY', 'https_proxy', 'HTTPS_PROXY', 'all_proxy', 'ALL_PROXY']:
    os.environ[env] = ''

response = urllib.request.urlopen("https://api.duckduckgo.com/?format=json&q=whats+my+ip",capath="/etc/ssl/certs")

print (re.sub('<[^>]*>','',json.loads(response.read().decode())['Answer']))
